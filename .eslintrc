{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "semi": "off",
    "no-unused-vars": "off",
    "object-curly-spacing": "off",
    "quote-props": "off",
    "max-len": ["error", { "code": 100 }],
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "semi-style": "off"
  }
}
